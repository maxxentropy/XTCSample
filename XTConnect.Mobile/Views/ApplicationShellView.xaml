<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             xmlns:mvvm="http://prismlibrary.com"
             xmlns:constants="clr-namespace:XTConnect.Core.Constants;assembly=XTConnect.Core"
             mvvm:ViewModelLocator.AutowireViewModel="Automatic"
             x:Class="XTConnect.Mobile.Views.ApplicationShellView">
    <ContentPage.Content>
        <ContentView prism:RegionManager.RegionName= "{ x:Static constants:PrismRegionNavConstants.ContentRegion }">
        <Button CornerRadius="0" HeightRequest="60" Text="SIGN IN" FontSize="20" FontAttributes="Bold" Command="{Binding NavigateCommand}" CommandParameter="{ x:Static constants:PrismRegionNavConstants.SignInPageView }" />
        </ContentView>
    </ContentPage.Content>
</ContentPage>